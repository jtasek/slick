{
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "no-invalid-this": 0,
        "no-magic-numbers": 0,
        "prefer-const": 0,
        "quotes": [2, "single"],
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/react-in-jsx-scope": 2,
        "react/sort-comp": 0,

        "strict": [2, "never"]
    },
    "extends": ["plugin:flowtype/recommended"],
    "plugins": ["babel", "react", "flowtype"]
}
